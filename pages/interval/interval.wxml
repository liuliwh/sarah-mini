<!--pages/interval/interval.wxml-->
<view class="container">
<form catchsubmit="dispatch">
  <input maxlength="-1" name="customRequest" placeholder="url_prefix" value="{{inputValue}}"/>
  <button form-type="submit" class="btn" data-type="set_url_prefix" >Set url_prefix</button>
  <button form-type="submit" class="btn" data-type="start" >Start</button>
  <button form-type="submit" class="btn" data-type="stop" >Stop</button>
  <button form-type="reset">clearInput</button>
</form>
<text>Echo result</text>
<scroll-view id="v_echo" scroll-x="true" scroll-y="true">
{{echo}}
</scroll-view>
</view>
